generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}

model User {
  id       Int    @id @default(autoincrement())
  username String @db.VarChar(255)
  email    String @unique @db.VarChar(255)
  password String @db.VarChar(255)
}
